#include <stdio.h>
#include <stdlib.h>


int main(int argc, char** argv) {
	
	int massiv[5]={5,4,10,4,5}; //массив размером 5 = числа массива
	int i;//счетчик
	int j;//счетчик
	int q;
	
	for (i=0; i<5-1;i++){//отнимаем единицу, чтобы правильно попадать в элементы массива
		for (j=0;j<5-i-1;j++){
			if (massiv[j]>massiv[j+1]){
				
				q=massiv[j];
				massiv[j]=massiv[j+1];
				massiv[j+1]=q;
				
			}
		}
	}
	
	for (i=0;i<5;i++){
		printf("%d ", massiv[i]);
	}
	
	return 0;
}
