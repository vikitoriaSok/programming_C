//ДЛИНА СТРОКИ  STRLEN() 

#include <stdio.h>
#include <string.h>
int main (void){
	char str[]="hello";
	printf("len str = %zu\n", strlen(str));
return 0;
}





//КОПИРОВАНИЕ СТРОК STRCOPY()

#include <stdio.h>
#include <string.h>
int main (void){	
	char str[] = "copy me";
	char dest[20];//место	
	strcpy(dest,str);
	printf("copy: %s\n", dest);	
return 0;
}
